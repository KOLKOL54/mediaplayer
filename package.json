{
	"dependencies": {
		"electron-log": "^5.4.3",
		"electron-updater": "^6.6.2",
		"express": "^5.1.0",
		"ffmpeg-static": "^5.2.0",
		"ffprobe-static": "^3.1.0",
		"fluent-ffmpeg": "^2.1.3"
	},
	"name": "mediaplayer",
	"version": "0.1.1",
	"description": "Caching media player",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "electron-builder",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --win"
	},
	"devDependencies": {
		"electron": "^38.3.0",
		"electron-builder": "^25.0.0"
	},
	"build": {
		"appId": "fi.k-net.mediaplayer",
		"productName": "MediaPlayer",
		"directories": {
			"output": "dist"
		},
		"files": [
			"**/*",
			"!dist",
			"!node_modules/.cache"
		],
		"win": {
			"icon": "app-icon.png",
			"target": "nsis"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true,
			"createDesktopShortcut": true,
			"createStartMenuShortcut": true
		},
		"publish": [
			{
				"provider": "github",
				"owner": "KOLKOL54",
				"repo": "mediaplayer"
			}
		]
	},
	"keywords": [],
	"author": "K-Net",
	"license": "Apache-2.0"
}
